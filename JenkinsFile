pipeline {
   agent any
   tools {
      maven 'Maven'
   }
   stages {
       stage("s1") {
                steps {
                    snDevOpsStep ()
                    echo "Building" 
                }
       }
      stage("s2") {
           steps {
               snDevOpsStep ()
                echo "Test" 
           }
        }
    
      stage("s3") {
             steps{
                  snDevOpsStep ()
                  echo "deploy in progress"       
                   snDevOpsChange()
              }
      }      
      
  }
}
